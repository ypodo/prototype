<?xml version="1.0"?>	
<!DOCTYPE cross-domain-policy SYSTEM "http://www.adobe.com/xml/dtds/cross-domain-policy.dtd">
<cross-domain-policy>
    <site-control permitted-cross-domain-policies="master-only"/>
    <allow-access-from domain="*.192.168.56.101:3000/"/>
    <allow-access-from domain="http://192.168.56.101:3000/"/>
    <allow-http-request-headers-from domain="*.google.com" headers="SOAPAction"/>
</cross-domain-policy>